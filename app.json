{
	"expo": {
		"name": "Shichida Memory Cards",
		"scheme": "shichida-memory-cards",
		"slug": "shichida-memory-cards",
		"version": "1.0.0",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "automatic",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#D3EDD4"
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"requireFullScreen": true,
			"supportsTablet": true,
			"bundleIdentifier": "com.dunghd.shichidamemorycards"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#D3EDD4"
			},
			"package": "com.dunghd.shichidamemorycards"
		},
		"web": {
			"bundler": "metro",
			"favicon": "./assets/favicon.png"
		},
		"plugins": [
			[
				"expo-font",
				{
					"fonts": ["./assets/fonts/CommitMono.otf"]
				}
			],
			[
				"expo-screen-orientation",
				{
					"initialOrientation": "LANDSCAPE"
				}
			],
			"expo-router"
		],
		"extra": {
			"eas": {
				"projectId": "2a90389b-c490-4a36-9108-03f4426c7002"
			}
		},
		"runtimeVersion": {
			"policy": "appVersion"
		},
		"updates": {
			"url": "https://u.expo.dev/2a90389b-c490-4a36-9108-03f4426c7002"
		}
	}
}
